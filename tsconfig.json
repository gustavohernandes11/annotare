{
    "compilerOptions": {
        "jsx": "react",
        "target": "es2016",
        "lib": ["DOM", "ES2016", "ESNext"],
        "module": "commonjs",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": false,
        "useUnknownInCatchVariables": true,
        "skipLibCheck": true
    }
}
